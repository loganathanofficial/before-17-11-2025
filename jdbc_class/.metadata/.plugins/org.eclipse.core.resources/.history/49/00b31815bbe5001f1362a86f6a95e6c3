package com.logu.insert_proj;

import java.util.Scanner;

public class data_deletion_into_table {
	public static void main(String[] args) {
		System.out.print("java performing crud operation with mysql server\n1.Insert\n2.Update\n3.delete\n\t\tChoose you any one :");
		Scanner sc= new Scanner(System.in);
		int choise=sc.nextInt();
		switch (choise) {
		case 1:
			try {
				Class.forName("com.logu.insert_proj.Insert_Operation");
			} catch (ClassNotFoundException e) {
				e.printStackTrace();
			}
			break;
		case 2:
			try {
				Class.forName("com.logu.insert_proj.Update_Operation");
			} catch (ClassNotFoundException e) {
				e.printStackTrace();
			}
			break;
		default:
			System.out.println("========your operation is not done try  again!!!========");
			break;
		}
		
	}
}
