public class FindLargeNuminArray{
	public static void main(String args[]){
		int arr[]={01,89,87,1,0,100};
		int max=arr[0];
		for(int i=1;i<arr.length;i++){
			System.out.println(max+"<"+arr[i]);
			if(max<arr[i]){
				max=arr[i];
			}
			
		}
		System.out.println("The maximun number is : "+max);
	}



}
